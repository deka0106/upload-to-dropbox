{
  "name": "upload-to-dropbox",
  "version": "2.0.1",
  "private": true,
  "description": "Upload files to Dropbox action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "package": "ncc build --source-map",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .js,.ts .",
    "lint:prettier": "prettier **/*.{js,ts} --check",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "test": "jest --passWithNoTests",
    "all": "run-s lint test build package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deka0106/upload-to-dropbox-action.git"
  },
  "keywords": [
    "actions",
    "dropbox"
  ],
  "author": "Deka <dekachan16@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.7",
    "dropbox": "^9.7.0",
    "globby": "^11.0.3",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@types/globby": "^9.1.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "@vercel/ncc": "^0.28.5",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  }
}
